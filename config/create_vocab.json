{
  "dataset_reader": {
    "type": "udify_universal_dependencies",
    "token_indexers": {
      "tokens": {
        "type": "single_id",
        "lowercase_tokens": false
      },
      "token_characters": {
        "type": "characters"
      }
    }
  },
  "vocabulary": {
    "non_padded_namespaces": ["upos", "xpos", "feats", "lemmas", "dep_heads", "dep_rels", "*tags", "*labels"],
    // Add the special "@@UNKNOWN@@" token to handle dev/test set labels that don't appear in training
    "tokens_to_add": {
      "upos": ["@@UNKNOWN@@"],
      "xpos": ["@@UNKNOWN@@"],
      "feats": ["@@UNKNOWN@@"],
      "lemmas": ["@@UNKNOWN@@"],
      "dep_heads": ["@@UNKNOWN@@"],
      "dep_rels": ["@@UNKNOWN@@"]
    }
  },
  "train_file_paths": {},
  "validation_file_paths": {}
}
